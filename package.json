{
  "name": "lcc-wifi-desktop",
  "productName": "LCC WiFi",
  "version": "1.0.0",
  "description": "Desktop application for LCC WiFi - Build on Electron",
  "main": "./src/main.js",
  "scripts": {
    "start": "electron src/main.js",
    "build-win": "electron-packager . --asar=true --overwrite --platform=win32 --arch=ia32 --icon=assets/icon.ico --out=release-builds",
    "build-linux": "electron-packager . --asar=true --overwrite --platform=linux --arch=x64 --icon=assets/icon.png --out=release-builds"
  },
  "author": "CF12",
  "license": "MIT",
  "devDependencies": {
    "electron-packager": "^8.6.0",
    "electron-prebuilt": "^1.4.13",
    "electron-reload": "^1.1.0",
    "standard": "^9.0.2"
  },
  "dependencies": {
    "electron": "^1.6.2",
    "electron-google-oauth": "^2.0.0",
    "firebase": "^3.7.5",
    "imagesloaded": "^4.1.1",
    "jquery": "^3.2.1",
    "node-fetch": "^1.6.3",
    "progressbar.js": "^1.0.1"
  }
}
